{
  "name": "fypnest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/fypnest/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/fypnest/test/jest-e2e.json",
    "yarn:cleanNode": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json && rm -rf .yarn && rm -rf .yarnrc.yml && rm -rf .yarn-integrity && rm -rf .yarnrc && rm -rf .pnp.*",
    "yarn:remove": "yarn cache clean && yarn install",
    "migrate:dev": "prisma migrate dev --name init",
    "migrate:up": "prisma db push",
    "migrate:dev:create": "prisma migrate dev --create-only ",
    "migrate:reset": "prisma migrate reset ",
    "migrate:deploy": "npx prisma migrate deploy ",
    "migrate:status": "npx prisma migrate status ",
    "migrate:resolve": "npx prisma migrate resolve ",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/graphql": "^10.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@paljs/plugins": "^5.2.0",
    "@prisma/client": "^4.10.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.29.3",
    "express": "^4.17.1",
    "graphql": "^16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.12.0",
    "moment-timezone": "^0.5.40",
    "nestjs-prisma": "^0.20.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma-nestjs-graphql": "^17.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.3.1",
    "rxjs": "^7.8.0",
    "uuid": "^7.0.0",
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/providers(|/.*)$": "<rootDir>/libs/providers/src/$1",
      "^@app/prisma-generated(|/.*)$": "<rootDir>/libs/prisma/src/$1"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
