    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "nx": {
    "targets": {}
  },
  "dependencies": {
    "@nestia/core": "^2.4.4",
    "@nestia/e2e": "^0.4.0",
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/platform-fastify": "^10.3.0",
    "@prisma/client": "^5.8.0",
    "axios": "^1.0.0",
    "fastify": "^4.25.2",
    "next": "13.4.4",
    "prisma": "^5.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "typia": "^5.3.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
